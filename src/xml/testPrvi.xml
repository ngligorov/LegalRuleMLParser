	xsi:schemaLocation="http://docs.oasis-open.org/legalruleml/ns/v1.0/ ../../schemas/xsd/xsd-compact/lrml-compact.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<lrml:Comment>clan47</lrml:Comment>

	<lrml:LegalSources>
