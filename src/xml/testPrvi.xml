				<ruleml:if>
					<ruleml:And key=":and1">
						<ruleml:Atom key=":atom1">
							<ruleml:Rel iri=":vozilo_nailazi_zdesna" />
							<ruleml:Var>X</ruleml:Var>
							<ruleml:Ind>vozilo_nailazi_zdesna</ruleml:Ind>
						</ruleml:Atom>
					</ruleml:And>
				</ruleml:if>
				<ruleml:then>
					<lrml:Prohibition>
